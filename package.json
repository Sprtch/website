    "moment": "^2.29.1",
    "pdfjs-dist": "^2.5.207",
    "polished": "^4.0.5",
    "react": "^17.0.2",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.2",
    "react-headroom": "^3.1.0",
    "react-helmet": "^6.1.0",
